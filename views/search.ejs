 <div class="container-fluid container-fluid-custom pb-5 mb-5">

    <div class="row row-col-2 row-cols-5 g-2 g-lg-3 ">
   
    <% if(searchResults != '')  { %>
    <% searchResults.forEach((note, index)=> { %>
        <a href="/item/<%= note._id %>">
<h4><%= note.title %></h4>
</a>

   <% }) %>
   <% } else { %>
<p>No Items Found</p>

    <% } %>
</div>

 </div>